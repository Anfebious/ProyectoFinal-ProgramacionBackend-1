<div id="container">
    <ul>
        {{#each products}}
        <li>{{#each this}}<strong>{{@key}}:</strong> {{this}}<br>{{/each}}</li>
        <button class = "product-btn" onclick="agregarAlCarrito('{{this._id}}')">Agregar al Carrito</button>
        {{/each}}
    </ul>
</div>
<button id="prev" onclick="getPrevPage()">Prev page</button>
<button id="next" onclick="getNextPage()">Next page</button>
<script src="/socket.io/socket.io.js"></script>
<script src="/js/index.js"></script>